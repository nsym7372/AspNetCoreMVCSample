@model IEnumerable<QuickMaster.Models.Book>
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

<table class="table">
    <thead>
        <tr>
            <th>書籍名</th>
            <th>価格</th>
            <th>出版社</th>
            <th>配布サンプル</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var i in Model)
        {
            <tr>
                <td>@i.Title</td>
                <td>@i.Price</td>
                <td>@i.Publisher</td>
                <td>@i.Sample</td>
            </tr>
        }
    </tbody>
</table>